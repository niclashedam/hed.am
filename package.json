{
  "devDependencies": {
    "@11ty/eleventy": "^3.1.2",
    "@11ty/eleventy-img": "^6.0.4",
    "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.2",
    "@shopify/prettier-plugin-liquid": "^1.9.4",
    "@tailwindcss/typography": "^0.5.16",
    "axe-core": "^4.10.3",
    "cspell": "^9.2.0",
    "glob": "^11.0.3",
    "html-minifier-terser": "^7.2.0",
    "jsdom": "^26.1.0",
    "prettier": "^3.3.3",
    "striptags": "^3.2.0",
    "tailwindcss": "^3.4.10"
  },
  "scripts": {
    "build": "eleventy",
    "check:format": "prettier . --check",
    "check:spelling": "npm run build && cspell --config cspell.json --no-progress \"public/**/*.html\"",
    "check:a11y": "npm run build && node ./scripts/a11y.js",
    "format": "prettier . --write",
    "serve": "eleventy --serve"
  }
}
