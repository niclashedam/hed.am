{
  "devDependencies": {
    "@11ty/eleventy": "^3.1.2",
    "@11ty/eleventy-img": "^6.0.4",
    "@11ty/eleventy-navigation": "^1.0.5",
    "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.2",
    "@shopify/prettier-plugin-liquid": "^1.10.0",
    "@tailwindcss/cli": "^4.1.18",
    "@tailwindcss/typography": "^0.5.19",
    "axe-core": "^4.11.1",
    "cspell": "^9.6.2",
    "glob": "^13.0.1",
    "html-minifier-terser": "^7.2.0",
    "jsdom": "^27.4.0",
    "jsonld": "^9.0.0",
    "prettier": "^3.8.1",
    "striptags": "^3.2.0",
    "tailwindcss": "^4.1.18"
  },
  "scripts": {
    "build": "npm run clean && eleventy",
    "check:format": "prettier . --check",
    "check:spelling": "npm run build && cspell --config cspell.json --no-progress \"public/**/*.html\"",
    "check:a11y": "npm run build && node ./scripts/a11y.js",
    "check:jsonld": "npm run build && node ./scripts/jsonld.js",
    "clean": "rm -rf ./public && git checkout public",
    "format": "prettier . --write",
    "serve": "eleventy --serve"
  },
  "packageManager": "yarn@4.10.0"
}
